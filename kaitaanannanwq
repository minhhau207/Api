function GetUi() 
    if getgenv().Tvk then
    if game.CoreGui:FindFirstChild("Silver Hub GUI") then
        for a, v in ipairs(game.CoreGui:GetChildren()) do
            if v.Name == "Silver Hub GUI" then
                v:Destroy()
            end
        end
    end
end
getgenv().Tvk = true
getgenv().Chon = true

